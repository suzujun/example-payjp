{
  "name": "example-payjp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www --optimize_for_size --max_old_space_size=700 --gc_interval=100",
    "test": "DEBUG=true:* ENV=test mocha `find ./test | grep -e '*.js' | grep -v tools | grep -v demo`",
    "lint": "eslint `find s test -name '*.js'`",
    "api": "node ./bin/www -m api",
    "genrev": "REV=`git rev-parse HEAD` && TM=`date '+%s000'` && echo '{\"commit\":\"'$REV'\",\"updatedAt\":'$TM'}' > rev.json",
    "tag": "npm version $1 -m \"Auto update: Changed the version to %s of the package.json\"",
    "version": "npm run genrev && git add ./rev.json",
    "postversion": "sh gen_tag.sh"
  },
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.1",
    "crypto-js": "^3.1.6",
    "eslint": "<2.3.0",
    "express": "~4.13.1",
    "express-validator": "^2.18.0",
    "fluent-logger": "^1.0.0",
    "jade": "^1.11.0",
    "jwt-simple": "^0.5.0",
    "lru-cache": "^4.0.0",
    "minimist": "^1.2.0",
    "moji": "^0.5.1",
    "moment": "^2.12.0",
    "morgan": "~1.7.0",
    "multer": "^1.1.0",
    "neo-async": "^1.7.1",
    "node-dogstatsd": "0.0.6",
    "payjp": "^1.2.0",
    "superagent": ">=1.8.0-beta.2",
    "underscore": "^1.8.3",
    "uuid4": "^1.0.0",
    "winston": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015-riot": "^1.0.3",
    "babelify": "^7.2.0",
    "chai": "^3.5.0",
    "mocha": "^2.3.4",
    "npm": "^3.8.7",
    "supertest": "^1.1.0"
  }
}
